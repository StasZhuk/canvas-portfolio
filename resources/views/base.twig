<!doctype html>
<html lang="ru">
<head>
   <meta charset="UTF-8">
   <meta name="viewport"
         content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
   <meta http-equiv="X-UA-Compatible" content="ie=edge">
   <link rel="icon" href="/favicon.ico" type="image/x-icon">
   <title>Markup</title>
   {% block stylesheet_all %}
      {% block stylesheets %}
      {% endblock %}
   {% endblock %}
</head>
<body>
{% block page_wrapper %}
   <div id="page-wrapper" class="page-wrapper">
      <div class="page-buffer">
         {% block header %}{% endblock %}
         {% block content %}{% endblock %}
         <div id="root"></div>
         <div class="page-buffer__end"></div>
      </div>
   </div>
{% endblock %}

{% block page_footer %}
   <div class="page-footer">
      {% block footer %}{% endblock %}
   </div>
{% endblock %}

<div class="page-helper"></div>

{% block modals_all %}
   {% include './modal/request.twig' %}
   {% include './modal/success.twig' %}
   {% block modals %}{% endblock %}
{% endblock %}

{% block javascript_all %}
   {% block javascripts %}{% endblock %}
{% endblock %}
</body>
</html>
{% import './macros/svg.twig' as svg %}
{% import './macros/form.twig' as form %}
{% import './macros/mixins.twig' as mixins %}
